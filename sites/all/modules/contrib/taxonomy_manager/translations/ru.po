msgstr "вперёд"
msgid "Select all"
msgstr "Выделить все"
msgid "Save changes"
msgstr "Сохранить изменения"
msgid "Down"
msgstr "Вниз"
msgid "Close"
msgstr "Закрыть"
msgid "Confirmation"
msgstr "Подтверждение"
msgid "CSV"
msgstr "CSV"
msgid "Search options"
msgstr "Параметры поиска"
msgid "Translations"
msgstr "Переводы"
